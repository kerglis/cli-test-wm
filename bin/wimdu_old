#!/bin/sh

# This is a stub to make the feature spec pass. You probably don't
# want to build this in shell script.

sleep 0.5 # simulates slow startup with ruby/bundler

case $1 in
    new)
        echo "Starting with new property ABC1DEF2."
        echo
        echo "Title: "
        read title
        echo "Address: "
        read address
        echo "Phone: "
        read phone
        ;;
esac
